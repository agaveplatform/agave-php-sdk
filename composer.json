{
    "name": "agaveplatform/agave-php-sdk",
    "description": "Client SDK to the Agave Platform",
    "type": "package",
    "keywords": [
        "agave",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://agaveapi.co",
    "license": "BSD 2-Clause",
    "authors": [
        {
            "name": "Rion Dooley",
            "homepage": "https://github.com/agaveplatform/agave-php-sdk"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Agave\\Client\\" : "src/Agave/Client/" }
    },
    "autoload-dev": {
        "psr-4": { "Agave\\Client\\" : "test/" }
    }
}
